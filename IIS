# Set the output path
$outputPath = "C:\inetpub\wwwroot\index.html"

# Set the content
$content = @"
<!DOCTYPE html>
<html>
<head>
    <title>Welcome to IIS</title>
</head>
<body>
    <h1>Hello from IIS</h1>
</body>
</html>
"@

# Write the content to the file
Set-Content -Path $outputPath -Value $content
